{
  env: {
    development: {
      presets: ["next/babel"],
      plugins: [
        "relay",
        [
          "styled-components",
          {
            ssr: true,
            displayName: true,
            preprocess: false
          }
        ]
      ]
    },
    production: {
      presets: ["next/babel"],
      plugins: ["relay"]
    },
    test: {
      presets: [["next/babel", { "preset-env": { modules: "commonjs" } }]],
      plugins: ["relay"]
    }
  }
}
